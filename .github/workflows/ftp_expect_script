#!/usr/bin/expect
spawn sftp -oHostKeyAlgorithms=+ssh-dss ftp.campbellluggblackwell.com
expect "Are you sure"
send "yes\r"
expect "Name (ftp.campbellluggblackwell.com:runner):"
send "[lindex $argv 0]@campbellluggblackwell.com\r"
expect "Password:"
send "[lindex $argv 1]\r"
expect "ftp>"
send "cd public_html\r"
expect "ftp>"
send "mget *.svg .\r"
expect "ftp>"
send "bye\r"
